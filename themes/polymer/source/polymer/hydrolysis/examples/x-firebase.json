{
  "polymer_modules": [],
  "elements": [
    {
      "is": "x-firebase",
      "events": {
        "data-change": "Fired when properties on `data` are added, removed, or modified.",
        "error": "Fired when an error occurs on an interaction with Firebase.  The\n `details.error` property contains the `Error` object provided by\n the Firebase API."
      },
      "properties": [
        {
          "name": "location",
          "type": "String",
          "desc": "Firebase location mapped to `data`."
        },
        {
          "name": "limit",
          "type": "Number",
          "desc": "Restricts the number of records reflected on the client."
        },
        {
          "name": "start",
          "type": "Number",
          "desc": "Specify a starting record for the set of records reflected on the client.."
        },
        {
          "name": "end",
          "type": "Number",
          "desc": "Specify an ending record for the set of records reflected on the client."
        },
        {
          "name": "data",
          "type": "Number",
          "desc": "The `data` object mapped to `location`.",
          "notify": true
        },
        {
          "name": "keys",
          "type": "Array",
          "desc": "All keys in data (array of names, if you think of data as a set of name/value pairs).",
          "notify": true
        },
        {
          "name": "childEvents",
          "type": "Boolean",
          "desc": "If true, will fire `child-added`, `child-removed`, `child-changed` events."
        },
        {
          "name": "priority",
          "type": "Number",
          "desc": "When set, data will be stored with the given Firebase priority level."
        },
        {
          "name": "dataReady",
          "type": "Boolean",
          "desc": "Restricts the number of records reflected on the client.",
          "notify": true
        },
        {
          "name": "log",
          "type": "Boolean",
          "desc": "If true, will log various occurances to the console api."
        }
      ],
      "methods": {
        "features": {},
        "locationChanged": {},
        "debouncedRequery": {},
        "requery": {},
        "queryChanged": {},
        "valueLoaded": {
          "args": [
            "snapshot"
          ]
        },
        "valueUpdated": {
          "args": [
            "snapshot"
          ]
        },
        "_updateData": {
          "args": [
            "data"
          ]
        },
        "observeQuery": {},
        "closeQuery": {},
        "observeData": {},
        "closeObserver": {},
        "dataChanged": {},
        "priorityChanged": {},
        "discardObservations": {},
        "deliverObservations": {},
        "childAdded": {
          "args": [
            "snapshot"
          ]
        },
        "childChanged": {
          "args": [
            "snapshot"
          ]
        },
        "childRemoved": {
          "args": [
            "snapshot"
          ]
        },
        "childEvent": {
          "args": [
            "kind",
            "snapshot"
          ]
        },
        "modulateData": {
          "args": [
            "operation",
            "snapshot"
          ]
        },
        "updateData": {
          "args": [
            "snapshot"
          ]
        },
        "removeData": {
          "args": [
            "snapshot"
          ]
        },
        "dataChange": {},
        "observeArray": {
          "args": [
            "splices"
          ]
        },
        "observeObject": {
          "args": [
            "added",
            "removed",
            "changed",
            "getOldValueFn"
          ]
        },
        "commitProperty": {
          "args": [
            "key"
          ]
        },
        "remove": {
          "args": [
            "key"
          ]
        },
        "commit": {},
        "push": {
          "args": [
            "item"
          ]
        },
        "errorHandler": {
          "args": [
            "error"
          ]
        }
      },
      "using": [],
      "scripts": [
        {
          "kind": "external",
          "src": "https://cdn.firebase.com/js/client/2.0.3/firebase.js"
        }
      ],
      "extends": null
    }
  ]
}